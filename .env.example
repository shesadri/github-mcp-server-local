# GitHub Personal Access Token
# Create one at: https://github.com/settings/personal-access-tokens/new
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here

# GitHub Toolsets (optional)
# Available: repos,issues,users,pull_requests,code_security,experiments
# Use 'all' to enable all toolsets
GITHUB_TOOLSETS=all

# Dynamic Toolsets (optional)
# Set to 1 to enable dynamic toolset discovery
GITHUB_DYNAMIC_TOOLSETS=0

# Server Configuration
PORT=3000